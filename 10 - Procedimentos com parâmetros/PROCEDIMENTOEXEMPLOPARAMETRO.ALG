Algoritmo "procedimentoExemploParametro"

procedimento soma(num1,num2:inteiro)
var
   aux: inteiro
inicio
      aux <- num1+num2
      res <- aux
fimprocedimento

Var

res, j, l, m, n: inteiro

Inicio
      j <- 33
      l <- 30
      m <- 4
      n <- 5
      soma(j,l)
      escreval (res)
      soma(m,n)
      escreval (res)
Fimalgoritmo